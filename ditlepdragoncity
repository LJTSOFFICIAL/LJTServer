<script>
    $(document).ready(function () {
        var sideSlider = $('[data-toggle=collapse-side]');
        var $sel = $(sideSlider.attr('data-target'));
        setTimeout(() => {

            $('.my-nav li.dropdown a').on('click', function (event) {
                $(this).parent().toggleClass('open');
            });
        }, 1000);

        sideSlider.click(function (event) {
            $sel.toggleClass('in');
            addRemoveSidebarClassOnBody();
        });

        $('.links a').click(function () {
            if ($(this).hasClass('dropdown-toggle'))
                return;
            closeSideBarMenu();
            addRemoveSidebarClassOnBody();
        });

        $('#mainContainer').on('touchend click', function (e) {
            if (e.target != sideSlider[0]) {
                if (!$sel.hasClass('in')) {
                    closeSideBarMenu();
                    addRemoveSidebarClassOnBody();
                    e.preventDefault();
                    return false;
                }
            }
        });

        //$('.side-collapse .overlay').click(function () {
        //    closeSideBarMenu();
        //    addRemoveSidebarClassOnBody();
        //});

        function addRemoveSidebarClassOnBody() {
            var $body = $('body');
            if ($sel.hasClass('in')) {
                $body.removeClass('side-bar-open');
                $body.addClass('side-bar-closed');
            }
            else {
                $body.removeClass('side-bar-closed');
                $body.addClass('side-bar-open');
            }
        }

        function closeSideBarMenu() {
            $sel.addClass('in');
        }
    });
</script> <nav class="navbar navbar-inverse with-shadow-new m-hide nav-desktop" ng-hide="false">
